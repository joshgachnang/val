{
  "name": "life-api",
  "version": "0.0.1",
  "description": "API for your life",
  "main": "server.js",
  "scripts": {
    "test": "mocha",
    "watch": "concurrently --kill-others 'npm run watch:scss' 'npm run watch:food' 'npm run watch:mirror' 'npm run watch:node'",
    "watch:scss": "node-sass -w food/scss/ -o public/css/",
    "watch:node": "nodemon -w . -w mirror server.js",
    "watch:food": "watchify food/js/*.js -o public/js/food.js -t [ babelify --presets [ es2015 ] ]",
    "watch:mirror": "watchify mirror/mirror.js -o public/js/mirror.js -t [ babelify --presets [ es2015 ] ]",
    "postinstall": "./extras/postinstall.sh",
    "clean": "rimraf *",
    "precompile": "npm run clean",
    "prebuild": "npm run build:clean",
    "build": "npm build:clean & npm build:food & build:mirror & build:scss",
    "build:clean": "rimraf public/css/*, public/js/*",
    "build:food": "browserify food/js/*.js -o public/js/food.js -t [ babelify --presets [ es2015 ] ]",
    "build:mirror": "browserify mirror/mirror.js -o public/js/mirror.js -t [ babelify --presets [ es2015 ] ]",
    "build:scss": "node-sass --include-path scss --source-map true food/scss/ -o public/css/",
    "start": "node server.js",
    "dev": "npm run watch",
    "bot": "cd bot && nodemon server.js"
  },
  "author": "Josh Gachnang",
  "license": "GPL-3.0",
  "dependencies": {
    "alexa-app": "^2.3.4",
    "alexa-app-server": "^2.2.4",
    "angular": "^1.4.9",
    "bcrypt-nodejs": "0.0.3",
    "better-require": "0.0.3",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.1",
    "coffee-script": "^1.10.0",
    "commander": "^2.3.0",
    "connect-flash": "^0.1.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.4.2",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "express-session": "^1.14.0",
    "extend": "^2.0.1",
    "glob": "^4.0.5",
    "google-auth-library": "^0.9.7",
    "googleapis": "^2.1.7",
    "hubot-scripts": "^2.16.2",
    "jade": "^1.11.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.11.1",
    "mathjs": "^3.3.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.4.14",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-uber": "0.0.7",
    "parse5": "^2.2.2",
    "passport": "^0.3.2",
    "passport-google-auth": "^1.0.1",
    "passport-local": "^1.0.0",
    "pluralize": "^3.0.0",
    "pug": "^2.0.0-alpha6",
    "q": "^1.4.1",
    "querystring": "^0.2.0",
    "request": "^2.56.0",
    "request-promise": "^0.4.2",
    "scoped-http-client": "^0.11.0",
    "slackbots": "^0.5.1",
    "underscore": "^1.8.3",
    "watchify": "^3.7.0",
    "winston": "^2.2.0",
    "ws": "^1.0.1",
    "xml2js-promise": "0.0.2",
    "xmldom": "^0.1.22",
    "xmlserializer": "^0.4.0",
    "xpath": "0.0.23"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "csscomb": "^3.1.7",
    "debowerify": "^1.5.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.4",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
