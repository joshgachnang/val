{
  "name": "veronica",
  "version": "0.1.0",
  "description": "A chat bot based on Hubot to automate your life",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:clean && npm run compile",
    "build:clean": "rm -rf built/*",
    "compile": "mkdir -p built/config/  && cp -a thirdParty built/ && cp envfile built/ | true && tsc",
    "docker": "npm run compile && docker build -t servercobra/veronica . && docker push servercobra/veronica",
    "dev": "npm run watch",
    "deploy": "npm run docker && npm run hyper",
    "hyper": "hyper stop veronica; hyper rm veronica; hyper pull servercobra/veronica; hyper run -d --name veronica -p 8080:8080 --env-file prodenvfile servercobra/veronica && hyper fip attach 199.245.57.103 veronica",
    "lint": "tslint adapters/*.ts plugins/*.ts test/*.ts *.ts",
    "mocha": "mocha built/test/*.js",
    "prebuild": "npm run build:clean",
    "pretty": "prettier --print-width 100 --trailing-comma 'all'  --write '**/*.ts'",
    "serve": "npm run watch:node",
    "start": "node built/server.js",
    "test": "npm run lint && npm run build && npm run mocha",
    "watch": "concurrently --kill-others 'npm run watch:typescript' 'npm run serve'",
    "watch:node": "nodemon built/server.js",
    "watch:typescript": "mocha-typescript-watch built/test/testPlugins.js"
  },
  "author": "Josh Gachnang",
  "license": "GPL-3.0",
