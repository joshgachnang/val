{
  "name": "val",
  "version": "0.1.0",
  "description": "A chat bot based on Hubot to automate your life",
  "main": "server.js",
  "scripts": {
    "app:serve": "cd app/ && npm run ionic:serve",
    "build": "npm run build:clean && npm run build:copy && npm run build:compile",
    "build:clean": "rm -rf dist/*",
    "build:compile": "tsc",
    "build:copy": "mkdir -p dist/config/ && cp -a thirdParty dist/ && if [ -f configuration.json  ]; then cp configuration.json  dist/; fi",
    "docker": "npm run build && docker build -t servercobra/val . && docker push servercobra/val",
    "precommit": "npm run precommit:lintfix && npm run precommit:pretty",
    "precommit:lintfix": "tslint --fix adapters/*.ts plugins/*.ts test/*.ts *.ts",
    "precommit:pretty": "prettier --print-width 100 --trailing-comma 'all'  --write '**/*.ts'",
    "start": "node dist/server.js",
    "test": "npm run test:lint && npm run build && npm run test:mocha",
    "test:lint": "tslint adapters/*.ts plugins/*.ts test/*.ts *.ts",
    "test:mocha": "mocha dist/test/*.js",
    "watch": "concurrently --kill-others 'npm run watch:typescript' 'npm run watch:node'",
    "watch:node": "nodemon dist/server.js",
    "watch:typescript": "mocha-typescript-watch dist/test/testPlugins.js"
  },
  "author": "Josh Gachnang",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "better-require": "0.0.3",
    "bluebird": "^2.10.2",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.1",
    "coffee-script": "^1.10.0",
    "commander": "^2.3.0",
    "connect-flash": "^0.1.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "cron": "^1.2.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "express-session": "^1.14.0",
    "extend": "^2.0.1",
    "fast-csv": "^2.3.0",
    "github": "^9.2.0",
    "glob": "^4.0.5",
    "google-auth-library": "^0.9.10",
    "googleapis": "^2.1.7",
    "hubot-scripts": "^2.16.2",
    "hubot-shipit": "^0.2.1",
    "js-combinatorics": "^0.5.0",
    "jsonwebtoken": "^7.1.9",
